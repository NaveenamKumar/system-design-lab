<clickhouse>
  <!-- Allow browser-based tools (e.g., Tabix) to query ClickHouse over HTTP from another origin. -->
  <http_server_default_response_headers>
    <Access-Control-Allow-Origin>*</Access-Control-Allow-Origin>
    <Access-Control-Allow-Methods>GET, POST, OPTIONS</Access-Control-Allow-Methods>
    <Access-Control-Allow-Headers>origin, x-requested-with, content-type, accept, authorization</Access-Control-Allow-Headers>
  </http_server_default_response_headers>
</clickhouse>

